{
  "swagger": "2.0",
  "info": {
    "title": "Solution API",
    "version": "1.0.0",
    "contact": {
      "name": "Dmitriy Burin",
      "email": "hahahihi@mail.com"
    }
  },
  "tags": [
    {
      "name": "SolutionV1"
    }
  ],
  "host": "localhost:8080",
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/solution/v1/deploy": {
      "post": {
        "summary": "Создание задачи на деплой",
        "operationId": "SolutionV1_Deploy",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/deployDeployResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/deployDeployRequest"
            }
          }
        ],
        "tags": [
          "SolutionV1"
        ]
      }
    },
    "/solution/v1/get_status": {
      "get": {
        "summary": "Получение статуса деплоя",
        "operationId": "SolutionV1_GetDeployStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/deployGetDeployStatusResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "SolutionV1"
        ]
      }
    },
    "/solution/v1/list": {
      "get": {
        "summary": "Получение доступных решений",
        "operationId": "SolutionV1_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/deployListResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "SolutionV1"
        ]
      }
    }
  },
  "definitions": {
    "deployDeployRequest": {
      "type": "object"
    },
    "deployDeployResponse": {
      "type": "object"
    },
    "deployGetDeployStatusResponse": {
      "type": "object"
    },
    "deployListResponse": {
      "type": "object",
      "properties": {
        "solutions": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/deploySolution"
          }
        }
      }
    },
    "deploySolution": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "info": {
          "$ref": "#/definitions/deploySolutionInfo"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "deploySolutionInfo": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "status": {
          "$ref": "#/definitions/deploySolutionStatus"
        }
      }
    },
    "deploySolutionStatus": {
      "type": "string",
      "enum": [
        "UNKNOWN"
      ],
      "default": "UNKNOWN"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
